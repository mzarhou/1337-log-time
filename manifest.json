{
    "name": "1337 log time",
    "version": "1.0.1",
    "description": "simple extension to calculate log time",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "96": "img/96.png",
        "128": "img/128.png"
      }
    },
    "icons": {
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png",
      "96": "img/96.png",
      "128": "img/128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.42.fr/*"],
            "js": ["script.js"],
            "run_at": "document_idle"
        }
    ],

    "manifest_version": 3
  }
